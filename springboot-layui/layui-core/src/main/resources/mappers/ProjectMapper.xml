    <insert id="insertBatch" keyProperty="id" useGeneratedKeys="true">
        insert into project(Number, Name, Type, BeginDate, EndDate, Director, Mebers, QuotaServiceState, Remark, State, Active, CreatedAt, CreatedBy, UpdatedAt, UpdatedBy)
        values
        <foreach collection="entities" item="entity" separator=",">
        (#{entity.number}, #{entity.name}, #{entity.type}, #{entity.begindate}, #{entity.enddate}, #{entity.director}, #{entity.mebers}, #{entity.quotaservicestate}, #{entity.remark}, #{entity.state}, #{entity.active}, #{entity.createdat}, #{entity.createdby}, #{entity.updatedat}, #{entity.updatedby})
        </foreach>
    </insert>
    

    

    <insert id="insertOrUpdateBatch" keyProperty="id" useGeneratedKeys="true">
        insert into project(Number, Name, Type, BeginDate, EndDate, Director, Mebers, QuotaServiceState, Remark, State, Active, CreatedAt, CreatedBy, UpdatedAt, UpdatedBy)
        values
        <foreach collection="entities" item="entity" separator=",">
            (#{entity.number}, #{entity.name}, #{entity.type}, #{entity.begindate}, #{entity.enddate}, #{entity.director}, #{entity.mebers}, #{entity.quotaservicestate}, #{entity.remark}, #{entity.state}, #{entity.active}, #{entity.createdat}, #{entity.createdby}, #{entity.updatedat}, #{entity.updatedby})
        </foreach>
        on duplicate key update
        Number = values(Number),
        Name = values(Name),
        Type = values(Type),
        BeginDate = values(BeginDate),
        EndDate = values(EndDate),
        Director = values(Director),
        Mebers = values(Mebers),
