        Remark = values(Remark),
        State = values(State),
        Active = values(Active),
        CreatedAt = values(CreatedAt),
        CreatedBy = values(CreatedBy),
        UpdatedAt = values(UpdatedAt),
        UpdatedBy = values(UpdatedBy)
    </insert>
    



